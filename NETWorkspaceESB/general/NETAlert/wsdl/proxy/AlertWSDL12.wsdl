<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="Logging" targetNamespace="http://www.netservicos.com.br/Alert" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/" xmlns:alert="http://www.netservicos.com.br/Alert" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:canalert="http://www.netservicos.com.br/modelocanonico/v2/alert">
	<wsdl:types>

		<xsd:schema targetNamespace="http://www.netservicos.com.br/Alert">

			<xsd:import schemaLocation="../../../../modelocanonicoV2/geral/Alert.xsd" namespace="http://www.netservicos.com.br/modelocanonico/v2/alert">
			</xsd:import>

			<xsd:element name="alert" type="canalert:Alert">
