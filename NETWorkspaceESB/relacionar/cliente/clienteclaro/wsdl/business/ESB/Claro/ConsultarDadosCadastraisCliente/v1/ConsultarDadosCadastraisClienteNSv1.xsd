			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="estado-civil" nillable="true" type="tns:ConsultarDadosCadastraisClientev1_EstadoCivilType">
				<xsd:annotation>
