											</xsd:pattern>
											<xsd:length value="3"></xsd:length>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:element>
								<xsd:element name="numeroTelefone" minOccurs="1" maxOccurs="1">
									<xsd:simpleType>
										<xsd:restriction base="xsd:string">
											<xsd:maxLength value="14"></xsd:maxLength>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:element>							
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="observacao" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:complexType>

