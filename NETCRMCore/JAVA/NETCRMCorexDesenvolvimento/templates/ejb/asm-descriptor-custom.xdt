      <security-role>
         <description><![CDATA[<XDtI18n:getString bundle="xdoclet.modules.ejb.XDocletModulesEjbMessages" resource="description_not_supported"/>]]></description>
         <role-name><XDtEjbSec:securityRoleName/></role-name>
      </security-role>
     </XDtEjbSec:forAllSecurityRoles>
    <XDtEjb:forAllBeans>
      <XDtClass:ifHasClassTag tagName="ejb.security-roles">
        <XDtClass:forAllClassTagTokens tagName="ejb.security-roles" paramName="role-names" delimiter=",">
         <security-role>
           <description><![CDATA[<XDtI18n:getString bundle="xdoclet.modules.ejb.XDocletModulesEjbMessages" resource="description_not_supported"/>]]></description>
           <role-name><XDtClass:currentToken/></role-name>
         </security-role>
       </XDtClass:forAllClassTagTokens>
      </XDtClass:ifHasClassTag>
    </XDtEjb:forAllBeans>

   <!-- method permissions -->
    <XDtMerge:merge file="ejb-method-permissions.ent">
     <!--
       To specify additional method-permission elements, add a file in the merge
