        </XDtClass:ifHasClassTag>
        <XDtClass:ifHasClassTag tagName="ejb:pk" paramName="method-intf">
         <method-intf><XDtClass:classTagValue tagName="ejb:pk" paramName="method-intf" values="Home,LocalHome"/></method-intf>
