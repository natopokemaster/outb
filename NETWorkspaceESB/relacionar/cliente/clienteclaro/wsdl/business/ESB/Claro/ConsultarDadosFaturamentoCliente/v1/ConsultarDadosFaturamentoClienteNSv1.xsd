<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<xsd:schema xmlns="http://www.w3.org/2001/XMLSchema"
		targetNamespace="http://www.claro.com.br/EBO/Claro/v1"
		xmlns:tns="http://www.claro.com.br/EBO/Claro/v1"
		xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		elementFormDefault="qualified" attributeFormDefault="qualified"
		version="1.0">

	<xsd:include schemaLocation="../../../../EBO/Claro/v1/ClienteNaoEncontradoFaultEBOv1.xsd" />
	<xsd:include schemaLocation="../../../../EBO/Claro/v1/ContaNaoEncontradaFaultEBOv1.xsd" />
	<xsd:include schemaLocation="../../../../EBO/Claro/v1/ParametroInvalidoFaultEBOv1.xsd" />
	<xsd:include schemaLocation="../../../../EBO/Claro/v1/TechnicalFaultEBOv1.xsd" />

<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_CpfType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Cadastro de Pessoas Físicas (CPF) - é o registro de um
				cidadão na Receita Federal brasileira no qual devem
				estar todos os contribuintes (pessoas físicas
				brasileiras ou estrangeiras com negócios no Brasil). O
				CPF armazena informações fornecidas pelo próprio
				contribuinte e por outros sistemas da Receita Federal.
				
				Exemplo de CPF: 22233344405
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{0,11}"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_RgType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Registro Geral (RG) - é o documento nacional de
				identificação civil no Brasil. é conhecido popularmente
				por cédula de identidade, carteira de identidade ou
				simplesmente RG .

				Sua emissão é de responsabilidade dos governos
				estaduais, entretanto a cédula de identidade tem
				validade em todo território nacional.
				
				Exemplo de RG: 123.456.789-0
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="numero" nillable="true" type="xsd:string"/><xsd:element maxOccurs="1" minOccurs="0" name="orgao-emissor" nillable="true" type="xsd:string"/></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_CategoriaType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa um tipo genérico para Categorias
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="categoria-id" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação da categoria
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_AssinanteType">

		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa um assinante do serviço de telefonia.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="mobile-assinante-id" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do assinante no sistema
						de billing (Mobile).
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<simpleType name="ConsultarDadosFaturamentoClientev1_TipoOcupacaoType">
		<annotation>
			<documentation>
				<![CDATA[
				Forma pela qual uma pessoa recebe seus rendimentos, que pode ser:
				
				EMPRESARIO -
				
				ASSALARIADO  -
				
				COMERCIANTE -
				
				APOSENTADO -
				
				PROFISSIONAL_LIBERAL -
				
				ESTUDANTE -
				
				DONA_DE_CASA -
				
				FUNCIONARIO_PUBLICO -
				
				AUTONOMO -
				
				MILITAR - 
				
				NAO_INFORMADO - 
				
							
				Exemplo de Tipo de Ocupação: ASSALARIADO
				]]>
			</documentation>
		</annotation>
		<restriction base="xsd:string">
			<xsd:enumeration value="EMPRESARIO"/>
			<xsd:enumeration value="ASSALARIADO"/>
			<xsd:enumeration value="COMERCIANTE"/>
			<xsd:enumeration value="APOSENTADO"/>
			<xsd:enumeration value="PROFISSIONAL_LIBERAL"/>
			<xsd:enumeration value="ESTUDANTE"/>
			<xsd:enumeration value="DONA_DE_CASA"/>
			<xsd:enumeration value="FUNCIONARIO_PUBLICO"/>
			<xsd:enumeration value="AUTONOMO"/>
			<xsd:enumeration value="MILITAR"/>
			<xsd:enumeration value="NAO_INFORMADO"/>
		</restriction>
	</simpleType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_FormaPagamentoType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Corresponde à forma de pagamento:

				BOLETO - Boleto de Cobrança

				DEBITO-AUTOMATICO - Débito-Automático
				
				Exemplo de forma de pagamento: BOLETO
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BOLETO"/>
			<xsd:enumeration value="DEBITO_AUTOMATICO"/>
			<xsd:enumeration value="DINHEIRO"/>
			<xsd:enumeration value="CHEQUE"/>
			<xsd:enumeration value="CARTAO_CREDITO"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_DadosCadastraisPFType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa os dados cadastrais de uma pessoa
				física.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="nome" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_NomePessoaType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Nome da pessoa.
						SPED: Obrigatório, tamanho máximo
						100 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="data-nascimento" nillable="true" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Data de nascimento da pessoa.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="nome-mae" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_NomePessoaType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Nome da mãe da pessoa.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="sexo" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_SexoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Sexo da pessoa.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="cpf" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_CpfType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						CPF da pessoa, tanto para brasileiros como
						estrangeiros. Cadastro Pessoa Física, código
						utilizado no Brasil para identificar de forma
						única pessoas físicas.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="rg" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_RgType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Documento de identificação da pessoa, apenas
						para brasileiros.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="passaporte" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Identificação do passaporte da pessoa.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="passaporte-pais-id" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do país conforme
						definido pelo banco central.
						SPED: Obrigatório para estrangeiros,
						tamanho máximo 5 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="contatos" type="tns:ConsultarDadosFaturamentoClientev1_ArrayOfContatoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Lista de contatos da pessoa.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_CicloFaturamentoType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa informações referentes ao ciclo de
				faturamento dos serviços Pós-Pago e Controle contratados
				por um
				cliente.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="codigo" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do ciclo de billing.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="dia-vencimento" nillable="true" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Dia de vencimento da conta.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="dia-fechamento" nillable="true" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Dia de fechamento do ciclo de billing.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_TelefoneType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Número de Telefone, corresponde a um número de telefone
				incluindo o código de área (DDD).
				
				A restrição aplicada a esse type significa que devem existir 
				entre 9 ou 16 caracteres que tenham valor entre 0 e 9 
				(neste caso pode começar com 0 por causa de números 0800, por exemplo).
				
				Exemplo de Telefone: 1194157781
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{9,16}"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_EnderecoType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa as informações que compõem um endereço.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="prefixo" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_TipoEnderecoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Prefixo do logradouro.
						SPED: Obrigatório quando não for caixa postal
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="logradouro" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Nome do logradouro.
						SPED: Obrigatório quando não
						for caixa postal, tamanho máximo 60 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="numero" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Número do logradouro.
						SPED: Obrigatório quando
						não for caixa postal, tamanho máximo 10 caracteres,
						quando logradouro não possuir número, utilizar texto "Sem Número"
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="complemento" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Informações complementares do logradouro.
						SPED:
						Opcional, tamanho máximo 60 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="bairro" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Bairro onde se localiza o endereço.
						SPED:
						Obrigatório quando não for caixa postal, tamanho máximo 60 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="caixa-postal" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Número da caixa-postal do endereço. Devem ser
						preenchidos a caixa-postal,
						a cidade, UF e cep.
						SPED: Obrigatório
						quando for caixa postal, tamanho máximo 8 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="cidade" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Cidade em que se localiza o endereço.
						SPED: Obrigatório, tamanho máximo 50 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="uf" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_UfType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Sigla da Unidade da Federeção em que se localiza
						o endereço.
						SPED: Obrigatório, tamanho máximo 2 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="cep" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_CepType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						CEP do endereço.
						SPED: Obrigatório, tamanho máximo 8 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_BancoType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa um banco, instituição financeira.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="banco-id" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do banco.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_SexoType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Corresponde ao gênero do indivíduo que pode ser:

				M - Masculino

				F - Feminino
				
				NAO_INFORMADO - utilizado apenas quando o campo for mandatorio 
				e não se obteve a informação
				
				Exemplo de Sexo: M
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="M"/>
			<xsd:enumeration value="F"/>
			<xsd:enumeration value="NAO_INFORMADO"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_SubTipoClienteType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Corresponde ao subtipo do cliente de modo a identificar clientes 
				com características de interesse para o negócio.
				
				NORMAL - cliente normal sem nenhuma característica especial ao 
				         negócio.
				TRIPLE A - cliente com características de especial interesse ao 
				         negócio.
				         
				PME - PEQUENAS E MEDIAS EMPRESAS
				
				GE - GRANDES EMPRESAS
				
				RASTREAMENTO - Rastreamento e ou Telemetria
				
				GOVERNO - Governo
				
				TOP_ACCOUNT - para clientes subtipo TOP_ACCOUNT
				
				CN - Contas Nacionais.
								
				AACE - 				         
				         
				ASSOCIACOES - Associações e/ou cooperativa
				
				TOP REGIONAL - É uma PME com maior valor agregado.

				ASSOCIADO - Representa uma pessoa associada à organização.
				FORMADOR_DE_OPINIAO - Representa uma pessoa que tem a capacidade de influenciar e modificar a opinião de outras pessoas.
				GRANDES_CLIENTES - Representa uma pessoa de influência para organização
				INDIO - Representa pessoa de Natureza Indígena
				MEMBRO_EMBAIXADA_CONSULADO - Representa pessoa filiada à Embaixada ou Consulado   
				PROFISSIONAL_LIBERAL - Representa o profissional que por conta própria e presta serviços na sua área de trabalho.
				COMBO_MULTI_NET - Representa Cliente com produtos/serviços combinados entre as Empresas Claro e NET (COMBO).
				COMBO_MULTI_EMBRATEL - Representa Cliente com produtos/serviços combinados entre as Empresas Claro e Embratel (COMBO).
				DENATRAN - Representa Cliente com produtos/serviços de telemetria para automóveis (DENATRAN).
	
				Exemplo de Sub Tipo de Cliente: NORMAL
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NORMAL"/>
			<xsd:enumeration value="TRIPLE_A"/>
			<xsd:enumeration value="PME"/>
			<xsd:enumeration value="GE"/>
			<xsd:enumeration value="RASTREAMENTO"/>
			<xsd:enumeration value="GOVERNO"/>
			<xsd:enumeration value="TOP_ACCOUNT"/>
			<xsd:enumeration value="CN"/>
			<xsd:enumeration value="AACE"/>
			<xsd:enumeration value="ASSOCIACOES"/>
			<xsd:enumeration value="TOP_REGIONAL"/>
			<xsd:enumeration value="ASSOCIADO"/> 
			<xsd:enumeration value="FORMADOR_DE_OPINIAO"/> 
			<xsd:enumeration value="GRANDES_CLIENTES"/> 
			<xsd:enumeration value="INDIO"/> 
			<xsd:enumeration value="MEMBRO_EMBAIXADA_CONSULADO"/> 
			<xsd:enumeration value="PROFISSIONAL_LIBERAL"/> 
			<xsd:enumeration value="COMBO_MULTI_NET"/> 
			<xsd:enumeration value="COMBO_MULTI_EMBRATEL"/> 
			<xsd:enumeration value="DENATRAN"/> 
		</xsd:restriction>
	</xsd:simpleType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_EmailType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				E-mail (Eletronic Mail) - é um método que permite
				compor, enviar e receber mensagens através de sistemas
				eletrônicos de comunicação. O termo e-mail é aplicado
				tanto aos sistemas que utilizam a Internet e são
				baseados no protocolo SMTP, como aqueles sistemas
				conhecidos como intranets, que permitem a troca de
				mensagens dentro de uma empresa ou organização e são,
				normalmente, baseados em protocolos proprietários.
				
				Exemplo de e-mail: glauco.bordin@claro.com.br
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\w+([\-+._]\w*)*@\w+([\-.]\w+)*\.\w+([\-.]\w+)*"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_CepType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Código de Endereçamento Postal (CEP) ou Código Postal -
				é um código desenvolvido pelas administrações postais, e
				criado no sentido de facilitar o encaminhamento e a
				entrega das correspondências. Cada administração postal
				é livre de criar o código que melhor se adapte à
				realidade postal e administrativa do seu país.

				O sistema de código postal brasileiro, o CEP (Código de
				Endereçamento Postal), foi criado em 1972, com cinco
				dígitos. Em 1992, com o aumento do volume de
				correspondência, passou ao formato 00000-000, dividido
				em dois grupos, sendo o primeiro de 5 dígitos e o
				segundo de 3, separados por um hífen. A única empresa
				que atua em todos os municípios do país é a empresa
				estatal Correios.

				Exemplo de endereçamento: 05425902
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{0,8}"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_ProdutoType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Corresponde aos tipos de produtos oferecidos pela Claro:

				Claro Cartao - Serviço de Pré-Pago

				Claro Conta - Serviço de Pós-Pago
				
				Claro Controle - Serviço híbrido, onde o assinante possui uma 
				assinantura e franquia de uso, como no Claro Conta, mas ao 
				ultrapassar a franquia, apresenta comportamento semelhante ao
				Claro Cartao
				
				Exemplo de Produto: CLARO_CARTAO
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CLARO_CARTAO"/>
			<xsd:enumeration value="CLARO_CONTA"/>
			<xsd:enumeration value="CLARO_CONTROLE"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_PlanoPrecoType">
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="tipo-plano" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_ProdutoType"/></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_ContatoType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa as informações de um contato, sob duas óticas distintas:
				
				1 - Contato no sentido de ser a pessoa cadastrada no sistema de CRM antes que ela se
				torne um cliente. Um exemplo de contato é o prospect. Dentre os atributos do contato
				estão o seu ID no sistema CRM (acrm-contato-id) e seus dados cadastrais de pessoa
				física (dados-cadastrais-pf). Ex:
				a) contato/acrm-contato-id
				
				2 - Contato no sentido de ser o meio de se contatar alguém, onde esse alguém pode ser
				o próprio contato (pessoa) ou um cliente (entidade ClienteType). O contato como meio
				se de contatar alguem é normalmente um atributo dos dados cadastrais. Ex:
				a) cliente/dados-cadastrais-pf/contatos/contato/telefone
				b) contato/dados-cadastrais-pf/contatos/contato/email. Aqui observamos a primeira parte 
				(contato/dados-cadastrais-pf) referente ao contato como sendo a pessoa, e a segunda parte
				(contatos/contato/email) referente ao contato como sendo o meio de contatar a pessoa.
				  
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="nome" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_NomePessoaType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Este elemento é relevante para o contato no sentido de ser o meio de se contatar uma pessoa.
						
						Nome da pessoa para entrar em contato, ou seja, a pessoa que representa a pessoa alvo.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="telefone" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_TelefoneType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Este elemento é relevante para o contato no sentido de ser o meio de se contatar uma pessoa.

						Número do telefone da pessoa de contato.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="email" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_EmailType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Este elemento é relevante para o contato no sentido de ser o meio de se contatar uma pessoa.

						E-mail da pessoa de contato.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_MercadoFaturamentoType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa um mercado para faturamento de clientes
				da Claro.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="mobile-mercado-id" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do mercado no Mobile.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="regional" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_MercadoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Regional a qual o mercado de faturamento
						pertence.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="uf" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_UfType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Sigla da Unidade da Federeção de abrangência do
						mercado de faturamento.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="area" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						�?rea do mercado de faturamento, por exemplo:
						CAPITAL, METROPOLITANA, INTERIOR, LITORAL, etc
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="localidade" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Nome da localidade (cidade ou município)
						pertencente ao
						o mercado de faturamento.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_ClienteType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa um cliente contratante de serviço de
				telefonia.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element default="PF" maxOccurs="1" minOccurs="0" name="tipo-pessoa" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_TipoPessoaType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Corresponde ao tipo de pessoa, se PF - pessoa
						física ou PJ - Pessoa Jurídica.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element default="NORMAL" maxOccurs="1" minOccurs="0" name="sub-tipo-cliente" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_SubTipoClienteType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Corresponde ao subtipo de pessoa, se é um
						cliente individual ou tripleA.

						@depreciado - Utilize o elemento
						mobile-cliente-subtipo
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="dados-cadastrais-pf" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_DadosCadastraisPFType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Dados cadastrais do cliente, apenas para
						clientes Pessoa Física.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="dados-socio-economicos-pf" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_DadosSocioEconomicosPFType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Dados sócio econômicos do cliente, apenas para
						cliente Pessoa Física.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="dados-cadastrais-pj" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_DadosCadastraisPJType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Dados cadastrais do cliente, apenas para Pessoa
						Jurídica.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="accounts" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_ArrayOfAccountType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Lista de contas de cobrança do cliente.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="senha-atendimento" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Senha de identificação do cliente junto à
						central de atendimento.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="contratos" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_ArrayOfContratoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Lista dos contratos do cliente
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="mobile-cliente-id" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do cliente no sistema de
						billing (Mobile).
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="mobile-cliente-tipo" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do tipo de cliente no
						sistema de billing (Mobile).
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="mobile-cliente-subtipo" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do subtipo de cliente no
						sistema de billing (Mobile).
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_TipoEnderecoType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Corresponde aos tipo de de endereço possíveis, segundo cadastro 
				dos correios:
				
				AEROPORTO
				ALAMEDA
				AREA
				AVENIDA
				CAMPO
				CHACARA
				COLONIA
				CONDOMINIO
				CONJUNTO
				DISTRITO
				ESPLANADA
				ESTACAO
				ESTRADA
				FAVELA
				FAZENDA
				FEIRA
				JARDIM
				LADEIRA
				LAGO
				LAGOA
				LARGO
				LOTEAMENTO
				MORRO
				NUCLEO
				PARQUE
				PASSARELA
				PATIO
				PRACA
				QUADRA
				RECANTO
				RESIDENCIAL
				RODOVIA
				RUA
				SETOR
				SITIO
				TRAVESSA
				TRECHO
				TREVO
				VALE
				VEREDA
				VIA
				VIADUTO
				VIELA
				VILA
				OUTROS
				
				Exemplo de Tipo de Endereço: RUA
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_AccountType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa a conta-corrente de um cliente junto à
				Claro.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="mobile-ban" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação da account no módulo de
						cobrança do sistema de pós-pago.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="mercado-faturamento" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_MercadoFaturamentoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Mercado de Faturamento ao qual a account do
						cliente pertence.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="situacao" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_SituacaoAccountType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Situação da account do cliente.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="data-situacao" nillable="true" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Data e hora em que a account assumiu a situação.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="motivo-situacao" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Motivo da account ter passado para a situação.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="adimplente" nillable="true" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						@depreciado - Utilize o elemento
						indicador-adimplencia.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="indicador-processo-judicial" nillable="true" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Indica se a conta está em processo judicial (este cenário é conhecido internamente como ACA).
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="classe-credito" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_ClasseCreditoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Classe de crédito a qual a conta pertence. Define o grau
						de confiança da conta em relação aos pagamentos de faturas
						da conta.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="data-alteracao-classe-credito" nillable="true" type="dateTime">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Data e hora da última alteração da classe de crédito
						desta conta.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="valor-limite-uso" nillable="true" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Valor monetário que representa o limite de
						crédito permitido para o cliente.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="valor-uso" nillable="true" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Valor monetário que representa a quantidade de
						crédito já utilizada para o cliente.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="valor-sugerido-pagamento-limite-uso" nillable="true" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Valor monetário sugerido para pagamento pelo
						cliente, para que seu limite de crédito 
						permitido seja aumentado para este ciclo de 
						faturamento. 
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="valores-nao-faturados" nillable="true" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
					Valor total de unbilled. Representa a soma dos valores
					consumidos pela conta, que ainda não foram processados 
					pelo Billing.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="valores-faturados-sem-pagamento" nillable="true" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
					Valor total faturado da conta que ainda não foi pago 
					pelo cliente.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="quantidade-pagamentos" nillable="true" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
					Número de vezes em que foi efetuado um pagamento para 
					a conta, desde a sua abertura.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="valor-total-pago" nillable="true" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
					Valor total pago para a conta, desde a sua ativação.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="data-ativacao" nillable="false" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Data de ativacao da conta. Corresponde ao
						campo START_OF_SERVICE no Mobile.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="categoria-emissao-fatura" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_CategoriaType">
                <xsd:annotation>
                       <xsd:documentation>
                       <![CDATA[
                             Representa a forma de emissão de fatura do Cliente. As opções possíveis são:
                             - UNIFICADA: Indica que o cliente possui produtos combinados entre as empresas do Grupo, e deseja a emissão de única fatura 
                                          contendo a unificação dos valores referentes a produtos/serviços contratados (Combo).
                             - DESMEMBRADA: Indica a emissão de fatura distinta contendo os valores referentes a produtos/serviços contratados de uma ou 
                                            mais empresas do Grupo. Toda fatura que possui produtos/serviços de uma única empresa será considerada com 
                                            emissão DESMEMBRADA.
                       ]]>
                       </xsd:documentation>
                </xsd:annotation>
            </xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_ArrayOfContatoType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa uma lista de pessoas de contato.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="unbounded" minOccurs="0" name="contato" type="tns:ConsultarDadosFaturamentoClientev1_ContatoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Lista de contatos.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_DadosSocioEconomicosPFType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa um conjunto de informações
				sócio-econômicas
				de uma pessoa física.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="estado-civil" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_EstadoCivilType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Estado civil da pessoa física.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="ocupacao" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_TipoOcupacaoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Forma pela qual uma pessoa recebe seus
						rendimentos
						Exemplo: EMPRESARIO, COMERCIANTE, ASSALARIADO
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="grau-instrucao" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Nível de escolaridade da pessoa.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="empregador" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Nome do empregador, caso a pessoa seja
						assalariado.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="cargo" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Cargo exercido pela pessoa.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="data-admissao" nillable="true" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Data de admissão da pessoa no emprego atual.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="faixa-salarial" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Faixa salarial da pessoa.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="quantidade-dependentes" nillable="true" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Número de dependentes (IRPF) declarados pela
						pessoa.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="indicador-contribuinte-fome-zero" nillable="true" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Indica se a pessoa física é contribuinte do
						programa fome zero e
						faz jus aos benefícios estabelecidos pela
						regulamentação.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="indicador-contribuinte-fundo-pobreza" nillable="true" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Indica se a pessoa física é contribuinte do
						programa fundo de pobreza
						e faz jus aos benefícios estabelecidos
						pela regulamentação.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="indicador-casa-propria" nillable="true" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Sinalização para indicar se a pessoa possui casa
						própria ou não.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_MercadoType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Corresponde aos mercados onde a Claro atua:

				Claro SP1 - São Paulo (Metropolitano) 

				Claro SP2 - São Paulo (Interior)
				
				Claro RJ/ES - Rio de Janeiro e Espírito Santo
				
				Claro CO - Centro-Oeste
				
				Claro BA/SE - Bahia e Sergipe
				
				Claro NE - Nordeste
				
				Claro PR/SC - Paraná e Santa Catarina
				
				Claro RS - Rio Grande do Sul
				
				Claro MG - Minas Gerais
				
				Claro NO - Norte
				
				Exemplo de Mercado: CLARO_SP1
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CLARO_SP1"/>
			<xsd:enumeration value="CLARO_SP2"/>
			<xsd:enumeration value="CLARO_RJES"/>
			<xsd:enumeration value="CLARO_CO"/>
			<xsd:enumeration value="CLARO_BASE"/>
			<xsd:enumeration value="CLARO_NE"/>
			<xsd:enumeration value="CLARO_PRSC"/>
			<xsd:enumeration value="CLARO_RS"/>
			<xsd:enumeration value="CLARO_MG"/>
			<xsd:enumeration value="CLARO_NO"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_SituacaoAccountType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Corresponde à situação da account do cliente, os valores 
				possíveis são:

				ABERTA - account aberto (Equiv. Mobile: VV_OPENED (O))
				
				TENTATIVA - account tentativo (Equiv. Mobile: VV_TENTATIVE (T))
				 
				CANCELADA - account cancelado (Equiv. Mobile: VV_CANCELLED (C))

				SUSPENSA - account suspenso (Equiv. Mobile: VV_FINAL_SUSPEND (F))
				
				Exemplo de Situação do Account: ABERTA 
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ABERTA"/>
			<xsd:enumeration value="TENTATIVA"/>
			<xsd:enumeration value="CANCELADA"/>
			<xsd:enumeration value="SUSPENSA"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_CnpjType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Cadastro Nacional da Pessoa Jurídica (CNPJ) - é um
				número único que identifica uma pessoa jurídica junto à
				Receita Federal brasileira (órgão do Ministério da
				Fazenda), necessário para que a pessoa jurídica tenha
				capacidade de fazer contratos e processar ou ser
				processada. O CNPJ veio substituir o CGC, Cadastro Geral
				de Contribuintes.

				Um número típico de CNPJ tem o formato:
				03.847.655/0001-98 onde os oito primeiros números formam
				a "raiz" - que identifica a empresa, os quatro seguintes
				formam o "sufixo" que identifica uma unidade de atuação
				de empresa, ou seja, um endereço de atividade da pessoa
				jurídica, e os dois últimos formam o "dígito
				verificador" que é resultado de uma equação com os doze
				números anteriores. Deste modo, o número acima
				exemplificado identifica a matriz da empresa.
				
				Exemplo de CNPJ: 40432544000147
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{0,15}"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_ArrayOfAccountType">
		<xsd:sequence><xsd:element maxOccurs="unbounded" minOccurs="0" name="account" type="tns:ConsultarDadosFaturamentoClientev1_AccountType">
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_ArrayOfContratoType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa uma lista de contratos.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="unbounded" minOccurs="0" name="contrato" type="tns:ConsultarDadosFaturamentoClientev1_ContratoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Lista de contratos.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_DadosCadastraisPJType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa os dados cadastrais de uma pessoa
				jurídica.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="razao-social" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Nome da razão social da pessoa jurídica.
						SPED: Obrigatório, tamanho máximo 100 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="nome-fantasia" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Nome de fantasia da pessoa jurídica.
						SPED: Obrigatório, tamanho máximo 100 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="cnpj" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_CnpjType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						CNPJ da pessoa jurídica. Cadastro Nacional de Pessoa Jurídica, 
						código utilizado para identificar de forma única empresas, 
						seja matriz ou filial no Brasil.
						SPED: Obrigatório
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="inscricao-estadual" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Número da inscrição estadual da pessoa jurídica, 
						junto à Secretaria da Fazenda do Estado onde a 
						empresa está constituída.
						SPED: Obrigatório, tamanho máximo 16 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="inscricao-municipal" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Número da inscrição municipal da pessoa
						jurídica.
						SPED: Obrigatório, tamanho máximo 14 caracteres
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="contatos" type="tns:ConsultarDadosFaturamentoClientev1_ArrayOfContatoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Lista de contatos da pessoa.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_MetaInfType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa informações referentes à aplicação que
				gerou
				a solicitação para o serviço. esta informações são úteis
				na
				identificação de problemas e são utilizadas para
				rastreamento de
				segurança.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="1" name="codigo-aplicacao-origem" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação da aplicação de origem.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="1" name="codigo-operacao-origem" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação da operação realizada na
						aplicação de origem.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="1" name="correlacao-id-origem" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação da transação na
						aplicação de origem.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="1" name="endereco-aplicacao-origem" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Endereço de origem da transação. Em geral contém
						o IP ou nome da máquina ge gerou a solicitação
						para o serviço.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="1" name="usuario-id-origem" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do usuário autenticado
						na aplicação de origem.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="data-hora-origem" nillable="true" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Data e hora em que o serviço foi solicitado pela
						aplicação de origem.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="canal-id-origem" nillable="true" type="string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código do canal de interação com o cliente (da aplicação) 
						em que a aplicação que originou a requisição funciona;
						pode assumir valores de canais de atendimento, 
						vendas e interação em geral, como por exemplo:
						
						SITE, URA, LOJAONLINE, TOTEM, RCV, etc. 
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<simpleType name="ConsultarDadosFaturamentoClientev1_TipoPessoaType">
		<annotation>
			<documentation>
				<![CDATA[
				Corresponde ao tipo de pessoa, que pode ser:

				PF - Pessoa Física 

				PJ - Pessoa Jurídica
				
				Exemplo de Tipo de Pessoa: PF
				]]>
			</documentation>
		</annotation>
		<restriction base="xsd:string">
			<enumeration value="PF"/>
			<enumeration value="PJ"/>
		</restriction>
	</simpleType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_FormaPagamentoContaType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Forma de pagamento da conta dos serviços de
				telefonia
				contratados.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="forma-pagamento" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_FormaPagamentoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Forma de pagamento da conta.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="endereco-fatura" type="tns:ConsultarDadosFaturamentoClientev1_EnderecoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Endereço para onde será enviada a fatura do
						serviço e o boleto de cobrança, caso essa seja o
						forma de
						pagamento.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="banco" nillable="true" type="tns:ConsultarDadosFaturamentoClientev1_BancoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Instituição bancária.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="agencia" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Agência bancária, caso a forma de pagamento seja
						débito automático.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="conta-corrente" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Número da conta-corrente bancária, caso a forma
						de pagamento seja débito automático.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_NomePessoaType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Nome de Pessoa corresponde ao nome de uma pessoa, que
				deve ser composto por pelo menos duas palavras.
				
				Exemplo de Nome de Pessoa: José da Silva
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_ContratoType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa um contrato de serviço realizado por um
				cliente de telefonia.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="ciclo-faturamento" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_CicloFaturamentoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						@depreciado - Utilize o elemento
						account.ciclo-faturamento

						Informações de data de
						vencimento e ciclo
						de
						faturamento estabelecidos para
						contratos de
						serviços Pós-Pagos e
						Controle.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="ciclo-faturamento-solicitado" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_CicloFaturamentoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						@depreciado - Utilize o elemento
						account.ciclo-faturamento-solicitado

						Informações do novo ciclo de
						faturamento
						solicitado para contratos de
						serviços Pós-Pagos e
						Controle.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="forma-pagamento-conta" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_FormaPagamentoContaType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						@depreciado - Utilize o elemento
						account.forma-pagamento-conta

						Forma de pagamento estabelecida para
						contratos
						de serviços Pós-Pagos e Controle.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="forma-pagamento-conta-solicitada" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_FormaPagamentoContaType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						@depreciado - Utilize o elemento
						account.forma-pagamento-conta-solicitada

						Nova forma de pagamento
						solicitada para
						contratos
						de serviços Pós-Pagos e Controle.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="assinantes" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_ArrayOfAssinanteType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Lista de assinantes relacionados a este
						contrato.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="plano-preco" nillable="false" type="tns:ConsultarDadosFaturamentoClientev1_PlanoPrecoType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Plano de preço no nível de contrato e que é
						aplicado a todos os assinantes associados a este
						contrato. Caso
						este plano de preço não esteja
						preenchido é porque cada assinante
						possui um
						plano de preço específico.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_ClasseCreditoType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Corresponde à classe de crédito do cliente, ou seja, ao grau de 
				confiança em relação aos pagamentos das faturas do cliente.
				
				Esta classificação evolui de acordo com os pagamentos efetuados, 
				os valores possíveis são:

				INDETERMINADA - Ainda não foi definida, situação inicial de um 
				                cliente.
				
				Exemplo de Classe de Crédito: INDETERMINADA 
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="INDETERMINADA"/>
			<xsd:enumeration value="BAIXO_RISCO"/>
			<xsd:enumeration value="MEDIO_RISCO"/>
			<xsd:enumeration value="ALTO_RISCO"/>
			<xsd:enumeration value="FRAUDE"/>
			<xsd:enumeration value="FORA_DE_CLASSIFICACAO"/>
			<xsd:enumeration value="PRATA"/>
			<xsd:enumeration value="OURO"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_EstadoCivilType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Estado Civil - é a situação de uma pessoa em relação ao
				matrimônio ou à sociedade conjugal.

				De acordo com as leis brasileiras, os possíveis estados
				civis são:

				SOLTEIRO(A) - quem nunca se casou ou que teve o
				casamento anulado

				CASADO(A) - quem contraiu matrimônio, independente do
				regime de bens adotado

				SEPARADO(A) judicialmente - quem não vive mais com o
				cônjuge (vive em separação física dele), mas que ainda
				não obteve o divórcio

				DIVORCIADO(A) - após a homologação do divórcio pela
				justiça

				VIúVO(A) - pessoa cujo cônjuge faleceu

				UNIãO ESTáVEL - condição de convivência considerada como
				entidade familiar não registrada (como o casamento),
				onde são exigidos o atendimento de quatro requisitos
				fundamentais: que a convivência seja duradoura, seja
				pública, seja contínua, e que a união tenha o objetivo
				de constituir família (Lei 9.278/96).
				
				NãO INFORMADO - usar para os sistemas em que este atributo é obrigaório
				e não se possue a informação.
				
				Exemplo de Estado Civil: SOLTEIRO
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SOLTEIRO"/>
			<xsd:enumeration value="CASADO"/>
			<xsd:enumeration value="SEPARADO"/>
			<xsd:enumeration value="DIVORCIADO"/>
			<xsd:enumeration value="VIUVO"/>
			<xsd:enumeration value="UNIAO_ESTAVEL"/>
			<xsd:enumeration value="NAO_INFORMADO"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:complexType name="ConsultarDadosFaturamentoClientev1_ArrayOfAssinanteType">
		<xsd:annotation>
			<xsd:documentation>
			<![CDATA[
				Representa uma lista de assinantes de serviço de
				telefonia.
			]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence><xsd:element maxOccurs="unbounded" minOccurs="0" name="assinante" type="tns:ConsultarDadosFaturamentoClientev1_AssinanteType">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Lista de assinantes de telefonia.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element></xsd:sequence>
	</xsd:complexType>
<xsd:simpleType name="ConsultarDadosFaturamentoClientev1_UfType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Unidade da Federação (UF) - é o nome atribuído a cada um
				dos 26 estados brasileiros e ao Distrito Federal.

				AC - Acre

				AL - Alagoas

				AM - Amazonas

				AP - Amapá

				BA - Bahia

				CE - Ceará

				DF - Distrito Federal

				ES - Espírito Santo

				GO - Goiás

				MA - Maranhão

				MG - Minas Gerais

				MS - Mato Grosso do Sul

				MT - Mato Grosso

				PA - Pará

				PB - Paraíba

				PE - Pernambuco

				PI - Piauí

				PR - Paraná

				RJ - Rio de Janeiro

				RN - Rio Grande do Norte

				RO - Rondônia

				RR - Roraima

				RS - Rio Grande do Sul

				SC - Santa Catarina

				SE - Sergipe

				SP - São Paulo

				TO - Tocantins
				
				Exemplo de UF: SP 
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AC"/>
			<xsd:enumeration value="AL"/>
			<xsd:enumeration value="AP"/>
			<xsd:enumeration value="AM"/>
			<xsd:enumeration value="BA"/>
			<xsd:enumeration value="CE"/>
			<xsd:enumeration value="ES"/>
			<xsd:enumeration value="GO"/>
			<xsd:enumeration value="MA"/>
			<xsd:enumeration value="MT"/>
			<xsd:enumeration value="MS"/>
			<xsd:enumeration value="MG"/>
			<xsd:enumeration value="PA"/>
			<xsd:enumeration value="PB"/>
			<xsd:enumeration value="PR"/>
			<xsd:enumeration value="PE"/>
			<xsd:enumeration value="PI"/>
			<xsd:enumeration value="RJ"/>
			<xsd:enumeration value="RN"/>
			<xsd:enumeration value="RS"/>
			<xsd:enumeration value="RO"/>
			<xsd:enumeration value="RR"/>
			<xsd:enumeration value="SC"/>
