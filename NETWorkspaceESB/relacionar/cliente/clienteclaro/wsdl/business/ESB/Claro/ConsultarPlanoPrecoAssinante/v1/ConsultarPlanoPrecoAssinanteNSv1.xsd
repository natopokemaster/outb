<xsd:simpleType name="ConsultarPlanoPrecoAssinantev1_ProdutoType">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
				Corresponde aos tipos de produtos oferecidos pela Claro:

				Claro Cartao - Serviço de Pré-Pago

				Claro Conta - Serviço de Pós-Pago
				
				Claro Controle - Serviço híbrido, onde o assinante possui uma 
				assinantura e franquia de uso, como no Claro Conta, mas ao 
				ultrapassar a franquia, apresenta comportamento semelhante ao
				Claro Cartao
				
				Exemplo de Produto: CLARO_CARTAO
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CLARO_CARTAO"/>
			<xsd:enumeration value="CLARO_CONTA"/>
			<xsd:enumeration value="CLARO_CONTROLE"/>
		</xsd:restriction>
	</xsd:simpleType>
<xsd:complexType name="ConsultarPlanoPrecoAssinantev1_PlanoPrecoType">
		<xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" name="mobile-oferta-id" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do plano de preço
						no
						Mobile
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="wpp-plano-preco-id" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do plano de preço
						no WPP
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="wa-plano-preco-id" nillable="true" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
					<![CDATA[
						Código de identificação do plano de preço
						no WA
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="nome" nillable="true" type="xsd:string"/><xsd:element maxOccurs="1" minOccurs="0" name="data-efetivacao" nillable="true" type="dateTime"/><xsd:element maxOccurs="1" minOccurs="0" name="data-expiracao" nillable="true" type="dateTime"/><xsd:element maxOccurs="1" minOccurs="0" name="data-efetivacao-venda" nillable="true" type="dateTime"/><xsd:element maxOccurs="1" minOccurs="0" name="data-expiracao-venda" nillable="true" type="dateTime"/><xsd:element maxOccurs="1" minOccurs="0" name="data-expiracao-beneficio" nillable="true" type="dateTime">
