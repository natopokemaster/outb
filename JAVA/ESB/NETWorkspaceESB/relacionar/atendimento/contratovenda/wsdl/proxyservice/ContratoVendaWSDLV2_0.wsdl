						</xsd:sequence>
	
	            	</xsd:extension>
	            </xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="PessoaFisicaType">
				<xsd:complexContent>
					<xsd:extension base="tns:AbstractPessoaType">
                        
		                <xsd:sequence>
							<xsd:element name="dataNascimento" type="xsd:date" minOccurs="0" maxOccurs="1" />
							<xsd:element name="CPF" minOccurs="0" maxOccurs="1">
								<xsd:simpleType>							
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="\d{10,11}" />
										<xsd:maxLength value="11" />
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="RG" minOccurs="0" maxOccurs="1">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="20" />
									</xsd:restriction>
								</xsd:simpleType>							
							</xsd:element>
							<xsd:element name="orgaoExpedidor" type="xsd:string" minOccurs="0" maxOccurs="1" />
							<xsd:element name="flagEstrangeiro" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
							<xsd:element name="passaporte" type="xsd:string" minOccurs="0" maxOccurs="1" />
							<xsd:element name="nomePai" type="xsd:string" minOccurs="0" maxOccurs="1" />
							<xsd:element name="nomeMae" type="xsd:string" minOccurs="0" maxOccurs="1" />
							<xsd:element name="sexo" maxOccurs="1" minOccurs="0"
								nillable="true">
								<xsd:annotation>
									<xsd:documentation>
										Sexo do cliente Masculino, Feminino e Indefinido
										no caso de cliente pessoa juridica
									</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="MASCULINO" />
										<xsd:enumeration value="FEMININO" />
										<xsd:enumeration value="NAO SE APLICA" />
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="identificacaoEstadoCivil" type="xsd:long" minOccurs="0" maxOccurs="1" />
							<xsd:element name="identificacaoProfissao" type="xsd:long" minOccurs="0" maxOccurs="1" />
							<xsd:element name="identificacaoEscolaridade" type="xsd:long" minOccurs="0" maxOccurs="1" />
						</xsd:sequence>
                        			
		            </xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>
			
			<xsd:complexType name="RegistroContatoType">
				<xsd:sequence>
					<xsd:element name="nomeContato" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="telefoneContato" minOccurs="0" maxOccurs="1">
						<xsd:complexType>
							<xsd:sequence>
