<?xml version="1.0" encoding="UTF-8"?>
<!-- 
  XML de configuracao de webservices, que sera utilizado pelo WebServiceFatory do framework.
  o classpath deste arquivo deve ser informado ao WebServiceFatory, exemplo:
  "META-INF/applicationContext-model-webservices.xml"
  
  Os properties do arquivos de configuracao de webservices estao externalizados
  como lib em NETModelConfigLib
 -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jee="http://www.springframework.org/schema/jee"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
						http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-2.0.xsd">
	
	<!-- properties do arquivo de configuração de webservices  -->
	<bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="locations">
		  <list>
		    <value>classpath:META-INF/enviromentConfig.properties</value>
		    <value>classpath:META-INF/webServicesConfig.properties</value>
		  </list>
		</property>
	</bean>
	
	<!-- Configuração do Webservice para Atualizar Quantidade de Equipamentos na Controladora 
	<bean id="Nome da interface que extende WebService"
		class="br.com.netservicos.framework.service.webservice.WebServiceClient"
		scope="prototype" >

		<property name="bindingStub" value="nome completo da classe gerada pelo axis que extende org.apache.axis.client.Stub"/>
		<property name="remoteInterface" value="nome completo da interface gerada pelo axis que extende do java.rmi.Remote"/>
		<property name="serviceName" value="serviceName obtido do WSDL"/>
		<property name="servicePort" ref="referenecia para o bean de port"/>
		<property name="targetNamespace" value="target namespace obtido do WSDL"/>
		<property name="serverDomainSecure" value="true"/>
		<property name="identifyUserByDatabase" value="true"/>
		<property name="applicationName" value="nome da aplicacao, que devera ser um dos relacionados no enum dentro do NetHeader.xsd" />
		<property name="serviceVersion" value="versao do servico consumido" />
		<property name="timeout" value="timeout de consumo de serviço" />
	</bean>
	
	<bean id="AtualizarQuantidadeEquipamentosControladoraWebServicePort" class="br.com.netservicos.framework.service.webservice.WebServicePort">
		<property name="portName" value="nome do portType obtido do WSDL" />
		<property name="endpointAddress" value="${referencia para propertie em webServicesConfig.properties que contem o endpoint do serviço}" />
	</bean>
	 -->


	<bean id="IncluirRequisicaoInternaWebService"
		class="br.com.netservicos.framework.service.webservice.WebServiceClient"
		scope="prototype" >

		<property name="bindingStub" value="br.com.netservicos.atendimento.requisicaointerna.ws.service.IncluirRequisicaoInternaV2_0ServiceStub"/>
		<property name="remoteInterface" value="br.com.netservicos.atendimento.registrocontato.ws.service.IncluirRegistroContatoV2_0Service"/>
		<property name="serviceName" value="IncluirRequisicaoInternaV2_0Service"/>
		<property name="servicePort" ref="IncluirRequisicaoInternaWebServicePort"/>
		<property name="targetNamespace" value="http://br/com/netservicos/atendimento/evento/requisicaointerna"/>
		<property name="serverDomainSecure" value="true"/>
		<property name="identifyUserByDatabase" value="true"/>
		<property name="applicationName" value="OTHER" />
		<property name="serviceVersion" value="2.0" />
		<property name="timeout" value="30" />
	</bean>
	
	<bean id="IncluirRequisicaoInternaWebServicePort" class="br.com.netservicos.framework.service.webservice.WebServicePort">
		<property name="portName" value="IncluirRequisicaoInternaV2_0SoapPort" />
		<property name="endpointAddress" value="${requisicaointerna.endpoint}" />
	</bean>
	
	
	<bean id="IncluirRegistroContatoWebService"
		class="br.com.netservicos.framework.service.webservice.WebServiceClient"
		scope="prototype" >

		<property name="bindingStub" value="br.com.netservicos.atendimento.registrocontato.ws.service.IncluirRegistroContatoV2_0ServiceStub"/>
		<property name="remoteInterface" value="br.com.netservicos.atendimento.registrocontato.ws.service.IncluirRegistroContatoV2_0Service"/>
		<property name="serviceName" value="IncluirRegistroContatoV2_0Service"/>
		<property name="servicePort" ref="IncluirRegistroContatoWebServicePort"/>
		<property name="targetNamespace" value="http://br/com/netservicos/atendimento/evento/registrocontato"/>
		<property name="serverDomainSecure" value="true"/>
		<property name="identifyUserByDatabase" value="true"/>
		<property name="applicationName" value="OTHER" />
		<property name="serviceVersion" value="2.0" />
		<property name="timeout" value="30" />
	</bean>
	
	<bean id="IncluirRegistroContatoWebServicePort" class="br.com.netservicos.framework.service.webservice.WebServicePort">
		<property name="portName" value="IncluirRegistroContatoV2_0SoapPort" />
		<property name="endpointAddress" value="${registrocontato.endpoint}" />
	</bean>	
	
