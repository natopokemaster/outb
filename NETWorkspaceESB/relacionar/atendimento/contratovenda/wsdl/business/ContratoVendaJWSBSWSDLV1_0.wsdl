            </simpleType>
          </element>
          <element maxOccurs="1" minOccurs="1" name="IdentificacaoCidade">
            <simpleType>
              <restriction base="string">
                <length value="5"/>
              </restriction>
            </simpleType>
          </element>
        </sequence>
      </complexType>
      <element name="IdentificadorContrato" type="tns:IdentificadorContratoType"/>
    </schema>
