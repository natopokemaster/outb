						Estado civil da pessoa física.
					]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element><xsd:element maxOccurs="1" minOccurs="0" name="ocupacao" nillable="true" type="tns:ConsultarDadosCadastraisClientev1_TipoOcupacaoType">
