<?xml version='1.0' encoding='UTF-8'?>
<weblogic-application
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.bea.com/ns/weblogic/90"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/j2ee_1_4.xsd http://www.bea.com/ns/weblogic/90 http://www.bea.com/ns/weblogic/90/weblogic-application.xsd">

	<security>
		<realm-name>myrealm</realm-name>
		<security-role-assignment>
			<role-name>ACESSO</role-name>
			<principal-name>ACESSO</principal-name>
		</security-role-assignment>
		<security-role-assignment>
			<role-name>SOLICITAR_EXTENSAO_FONE</role-name>
			<principal-name>SOLICITAR_EXTENSAO_FONE</principal-name>
		</security-role-assignment>		
		<security-role-assignment>
			<role-name>ALTERACAO_DADOS_CADASTRAIS_LIVRES</role-name>
			<principal-name>ALTERACAO_DADOS_CADASTRAIS_LIVRES</principal-name>
		</security-role-assignment>
		<security-role-assignment>
			<role-name>ALTERACAO_DADOS_CADASTRAIS_RESTRITOS</role-name>
			<principal-name>ALTERACAO_DADOS_CADASTRAIS_RESTRITOS</principal-name>
		</security-role-assignment>
		<security-role-assignment>
			<role-name>ALTERACAO_DADOS_CONTATO</role-name>
			<principal-name>ALTERACAO_DADOS_CONTATO</principal-name>
		</security-role-assignment>
		<security-role-assignment>
			<role-name>ALTERAR_DADO_COBRANCA_CONTRATO</role-name>
			<principal-name>ALTERAR_DADO_COBRANCA_CONTRATO</principal-name>
		</security-role-assignment>
		<security-role-assignment>
			<role-name>ALTERAR_DADO_COBRANCA_PROPOSTA</role-name>
			<principal-name>ALTERAR_DADO_COBRANCA_PROPOSTA</principal-name>
		</security-role-assignment>
		<security-role-assignment>
			<role-name>INCLUIR_INTEGRACAO_MANIFESTACAO</role-name>
			<principal-name>INCLUIR_INTEGRACAO_MANIFESTACAO</principal-name>
		</security-role-assignment>
	</security>
	<classloader-structure>
		<classloader-structure>
			<module-ref>
				<module-uri>NETCRMCoreWebServices.war</module-uri>
			</module-ref>
		</classloader-structure>
	</classloader-structure>

