					<xsd:element name="dataAberturaEstabelecimento" type="xsd:date" minOccurs="0" maxOccurs="1" />
					<xsd:element name="porteSerpro" type="xsd:string" minOccurs="0" maxOccurs="1" />
					<xsd:element name="indicadorOpcaoSimples" type="xsd:boolean" minOccurs="0" maxOccurs="1" />
					<xsd:element name="situacaoSuframa" type="xsd:string" minOccurs="0" maxOccurs="1" />
					<xsd:element name="dataSituacaoSuframa" type="xsd:date" minOccurs="0" maxOccurs="1" />
					<xsd:element name="indicadorInternacional" type="xsd:boolean" minOccurs="0" maxOccurs="1" />
					<xsd:element name="classeConsumo" type="xsd:string" minOccurs="0" maxOccurs="1" />
					<xsd:element name="naturezaAssinante" type="xsd:string" minOccurs="0" maxOccurs="1" />
					<xsd:element name="tipoCliente" type="xsd:string" minOccurs="0" maxOccurs="1" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:element name="clienteProspect" type="cle:clienteProspectType" />
			<xsd:element name="endereco" type="cle:enderecoType" />
			<xsd:element name="identificador" type="cle:identificadorType" />
			<xsd:element name="descricao" type="cle:descricaoType" />
			<xsd:element name="cnae" type="cle:cnaeType" />
			<xsd:element name="consultarClienteBvcRequest" type="cle:consultarClienteBvcRequestType" />
      		<xsd:element name="consultarClienteBvcResponse" type="cle:consultarClienteBvcResponseType" />
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="consultarClienteBvcRequest">
		<wsdl:part element="cle:consultarClienteBvcRequest" name="payload" />
