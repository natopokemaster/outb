      </xs:element>
      <xs:element name="incluirRequisicaoInternaResponse">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="even:RequisicaoInterna" xmlns:even="http://www.netservicos.com.br/atendimento/evento"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:schema>
