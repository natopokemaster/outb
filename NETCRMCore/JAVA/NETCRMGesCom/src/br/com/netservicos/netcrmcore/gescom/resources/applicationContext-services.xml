<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jee="http://www.springframework.org/schema/jee"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
								http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-2.0.xsd"
								default-lazy-init="true">
	
	<!-- Start ReciboRelacionamentoService -->
	<alias alias="ReciboRelacionamentoService" name="ReciboRelacionamentoEJB"/>
	
	<jee:remote-slsb id="ReciboRelacionamentoEJB" 
		jndi-name="netcrmcore/gescom/ejb/ReciboRelacionamentoEJB"
		business-interface="br.com.netservicos.netcrmcore.gescom.reciborelacionamento.facade.ReciboRelacionamentoService"
		cache-home="true"
		lookup-home-on-startup="false" 
		resource-ref="false"
		refresh-home-on-connect-failure="true"/>
	<!-- End ReciboRelacionamentoService -->
	
	<!-- Start AvisoRelacionamentoService -->
	<alias alias="AvisoRelacionamentoService" name="AvisoRelacionamentoEJB"/>
	
	<jee:remote-slsb id="AvisoRelacionamentoEJB" 
		jndi-name="netcrmcore/gescom/ejb/AvisoRelacionamentoEJB"
		business-interface="br.com.netservicos.netcrmcore.gescom.avisorelacionamento.facade.AvisoRelacionamentoService"
		cache-home="true"
		lookup-home-on-startup="false" 
		resource-ref="false"
		refresh-home-on-connect-failure="true"/>
	<!-- End AvisoRelacionamentoService -->

</beans>