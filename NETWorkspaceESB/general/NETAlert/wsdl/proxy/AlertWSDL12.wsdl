			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="Alert">
		<wsdl:port binding="alert:AlertSOAP" name="AlertSOAP">
			<soap:address location="http://servbus/general/Alert12"/>
		</wsdl:port>
	</wsdl:service>
