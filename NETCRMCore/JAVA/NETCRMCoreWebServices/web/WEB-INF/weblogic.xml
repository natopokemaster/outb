<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-web-app
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:wls="http://www.bea.com/ns/weblogic/90"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd http://www.bea.com/ns/weblogic/90 http://www.bea.com/ns/weblogic/90/weblogic-web-app.xsd">


	<wls:security-role-assignment>
		<wls:role-name>ACESSO</wls:role-name>
		<wls:principal-name>ACESSO</wls:principal-name>
	</wls:security-role-assignment>

	<wls:security-role-assignment>
		<wls:role-name>SOLICITAR_EXTENSAO_FONE</wls:role-name>
		<wls:principal-name>SOLICITAR_EXTENSAO_FONE</wls:principal-name>
	</wls:security-role-assignment>

	<wls:security-role-assignment>
		<wls:role-name>ALTERACAO_DADOS_CADASTRAIS_LIVRES</wls:role-name>
		<wls:principal-name>ALTERACAO_DADOS_CADASTRAIS_LIVRES</wls:principal-name>
	</wls:security-role-assignment>

	<wls:security-role-assignment>
		<wls:role-name>ALTERACAO_DADOS_CADASTRAIS_RESTRITOS</wls:role-name>
		<wls:principal-name>ALTERACAO_DADOS_CADASTRAIS_RESTRITOS</wls:principal-name>
	</wls:security-role-assignment>

	<wls:security-role-assignment>
		<wls:role-name>ALTERACAO_DADOS_CONTATO</wls:role-name>
		<wls:principal-name>ALTERACAO_DADOS_CONTATO</wls:principal-name>
	</wls:security-role-assignment>

	<wls:security-role-assignment>
		<wls:role-name>INCLUIR_INTEGRACAO_MANIFESTACAO</wls:role-name>
		<wls:principal-name>INCLUIR_INTEGRACAO_MANIFESTACAO</wls:principal-name>
	</wls:security-role-assignment>
	
	<!-- Faz redirecionamento para URL original apÃ³s autenticar -->	
	<wls:container-descriptor>
		<wls:retain-original-url>true</wls:retain-original-url>
	</wls:container-descriptor>

	<wls:context-root>netcrmcorews</wls:context-root>
	
	<wls:session-descriptor>
        <wls:timeout-secs>60</wls:timeout-secs>
        <wls:persistent-store-type>replicated_if_clustered</wls:persistent-store-type>
        <wls:url-rewriting-enabled>false</wls:url-rewriting-enabled>
        <wls:encode-session-id-in-query-params>false</wls:encode-session-id-in-query-params>
   </wls:session-descriptor>
	
</wls:weblogic-web-app>