<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema 
	targetNamespace="http://www.netservicos.com.br/modelocanonico/v2/macroprocesso" 
	elementFormDefault="qualified"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:macroprocesso="http://www.netservicos.com.br/modelocanonico/v2/macroprocesso">	

	
	<xsd:simpleType name="MacroProcesso">
        <xsd:annotation>
        	<xsd:documentation>
        		Macro Processos de negócio da NET. Os valores possíveis são:
        		ARRECADAR, ATIVAR, FATURAR, INFRAESTRUTURA, RELACIONAR,
        		VENDER, ENTERPRISE.
        	</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
			<xsd:enumeration value="ARRECADAR"></xsd:enumeration>
			<xsd:enumeration value="ATIVAR"></xsd:enumeration>
			<xsd:enumeration value="FATURAR"></xsd:enumeration>
			<xsd:enumeration value="INFRAESTRUTURA"></xsd:enumeration>
			<xsd:enumeration value="RELACIONAR"></xsd:enumeration>
			<xsd:enumeration value="VENDER"></xsd:enumeration>
			<xsd:enumeration value="ENTERPRISE"></xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	
    <xsd:element name="MacroProcesso" type="macroprocesso:MacroProcesso"></xsd:element>
	
</xsd:schema>