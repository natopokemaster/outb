			<xsd:complexType name="TelefoneContatoType">
				<xsd:sequence>
					<xsd:element name="dddTelefone" minOccurs="1" maxOccurs="1">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:pattern value="\d{3}"></xsd:pattern>
								<xsd:minLength value="3"></xsd:minLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="numeroTelefone" minOccurs="1" maxOccurs="1">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="14"></xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>						
					<xsd:element name="ramal" minOccurs="0" maxOccurs="1">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="4"></xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>					
					</xsd:element>
					<xsd:element name="tipoTelefone" minOccurs="1" maxOccurs="1">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="RESIDENCIAL" />
								<xsd:enumeration value="COMERCIAL" />
								<xsd:enumeration value="CELULAR" />
								<xsd:enumeration value="FAX" />
								<xsd:enumeration value="OUTROS" />
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="EmailType">
				<xsd:sequence>
					<xsd:element name="enderecoEmail" minOccurs="0" maxOccurs="1">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:pattern
									value="[A-Za-z0-9!#-'\*\+\-/=\?\^_`\{-~]+(\.[A-Za-z0-9!#-'\*\+\-/=\?\^_`\{-~]+)*@[A-Za-z0-9!#-'\*\+\-/=\?\^_`\{-~]+(\.[A-Za-z0-9!#-'\*\+\-/=\?\^_`\{-~]+)*">
								</xsd:pattern>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="tipoEmail" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="aceitaContato" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="AbstractPessoaType" abstract="true">
			</xsd:complexType>

			<xsd:complexType name="PessoaJuridicaType">
				<xsd:complexContent>
